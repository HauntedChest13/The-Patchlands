{
  "multipart": [
    {
      "when": {
        "hanging": true,
        "flickering": false
      },
      "apply": {
        "model": "the_patchlands:block/ignisteel_lantern_hanging"
      }
    },
    {
      "when": {
        "hanging": true,
        "flickering": true
      },
      "apply": {
        "model": "the_patchlands:block/ignisteel_lantern_hanging_off"
      }
    },
    {
      "when": {
        "hanging": false,
        "flickering": true
      },
      "apply": {
        "model": "the_patchlands:block/ignisteel_lantern_off"
      }
    },
    {
      "when": {
        "hanging": false,
        "flickering": false
      },
      "apply": {
        "model": "the_patchlands:block/ignisteel_lantern"
      }
    }
  ]
}